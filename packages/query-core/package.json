    "peerDependencies": {
        "@tanstack/react-query": ">=5.0.0",
        "react": ">=19.0.0",
        "react-dom": ">=19.0.0"
    },
    "devDependencies": {
        "@tanstack/react-query": "^5.0.0",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "typescript": "^5.8.3"
    },
    "author": "DJ (TTT Productions)",
    "license": "MIT"
